{
  "name": "github-trend",
  "type": "module",
  "version": "1.2.0",
  "private": true,
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=6.0.0"
  },
  "scripts": {
    "dev": "nuxt dev",
    "test": "vitest",
    "update:data": "esno ./scripts/index.ts",
    "send": "esno ./scripts/sendEmail.ts",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "vercel": "nuxt build && nuxt generate --vercel",
    "ps:gitee": "esno scripts/pushGiteePage.ts",
    "up:gitee": "esno scripts/giteePage.ts",
    "pb:gitee": "pnpm run ps:gitee && pnpm run up:gitee",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lintf": "eslint . --fix",
    "preinstall": "npx only-allow pnpm",
    "prepare": "simple-git-hooks",
    "release": "standard-version",
    "clean:cache": "rimraf .eslintcache && rimraf node_modules && pnpm install"
  },
  "dependencies": {
    "@nuxt-alt/proxy": "^2.5.8",
    "@nuxtjs/robots": "^3.0.0",
    "@nuxtjs/sitemap": "^5.1.4",
    "@plugin-web-update-notification/vite": "^1.7.1",
    "@unocss/reset": "^0.59.4",
    "axios": "^1.6.8",
    "dayjs": "^1.11.10",
    "echarts": "^5.5.0",
    "fs-extra": "^11.2.0",
    "nodemailer": "^6.9.13",
    "octokit": "^3.2.0",
    "puppeteer": "^22.7.0",
    "sass": "^1.75.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.15.0",
    "@element-plus/nuxt": "^1.0.9",
    "@nuxt/devtools": "^1.2.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.12.7",
    "@types/nodemailer": "^6.4.14",
    "@unocss/eslint-config": "^0.59.4",
    "@unocss/nuxt": "^0.59.4",
    "@unocss/preset-rem-to-px": "^0.59.4",
    "@vueuse/core": "^10.9.0",
    "@vueuse/nuxt": "^10.9.0",
    "autoprefixer": "^10.4.19",
    "bumpp": "^9.4.0",
    "cheerio": "1.0.0-rc.12",
    "cssnano": "^6.1.2",
    "element-plus": "^2.7.1",
    "esno": "^4.7.0",
    "execa": "^8.0.1",
    "lint-staged": "^15.2.2",
    "node-fetch": "^3.3.2",
    "nuxt": "^3.11.2",
    "nuxt-icon": "^0.6.10",
    "postcss": "^8.4.38",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.11.1",
    "standard-version": "^9.5.0",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
