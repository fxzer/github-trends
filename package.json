{
  "name": "github-trend",
  "type": "module",
  "version": "2.0.0",
  "private": true,
  "engines": {
    "node": ">=16.20.0"
  },
  "scripts": {
    "dev": "nuxi dev -o",
    "test": "vitest",
    "update:data": "esno ./scripts/index.ts",
    "send": "esno ./scripts/sendEmail.ts",
    "build": "nuxi build",
    "generate": "nuxi generate",
    "vercel": "nuxi generate --vercel",
    "ps:gitee": "esno scripts/pushGiteePage.ts",
    "up:gitee": "esno scripts/giteePage.ts",
    "pb:gitee": "pnpm run ps:gitee && pnpm run up:gitee",
    "preview": "nuxi preview",
    "postinstall": "nuxi prepare",
    "lint": "eslint .",
    "lintf": "eslint . --fix",
    "preinstall": "npx only-allow pnpm",
    "prepare": "simple-git-hooks",
    "release": "standard-version",
    "clean:cache": "rimraf .eslintcache && rimraf node_modules && pnpm install"
  },
  "dependencies": {
    "@nuxt-alt/proxy": "^2.5.8",
    "@nuxtjs/robots": "^3.0.0",
    "@nuxtjs/sitemap": "^5.2.2",
    "@unocss/reset": "^0.61.0",
    "@vercel/analytics": "^1.3.1",
    "axios": "^1.7.2",
    "echarts": "^5.5.1",
    "fs-extra": "^11.2.0",
    "nodemailer": "^6.9.14",
    "octokit": "^4.0.2",
    "puppeteer": "^22.12.1",
    "unplugin-icons": "^0.19.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.2",
    "@iconify/json": "^2.2.223",
    "@nuxt/devtools": "^1.3.7",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.9",
    "@types/nodemailer": "^6.4.15",
    "@unocss/eslint-config": "^0.61.0",
    "@unocss/nuxt": "^0.61.0",
    "@unocss/preset-rem-to-px": "^0.61.0",
    "@vueuse/core": "^10.11.0",
    "@vueuse/nuxt": "^10.11.0",
    "bumpp": "^9.4.1",
    "cheerio": "1.0.0-rc.12",
    "cssnano": "^7.0.3",
    "esno": "^4.7.0",
    "execa": "^9.3.0",
    "lint-staged": "^15.2.7",
    "node-fetch": "^3.3.2",
    "nuxt": "^3.12.2",
    "postcss": "^8.4.38",
    "rimraf": "^5.0.7",
    "sass": "^1.77.6",
    "simple-git-hooks": "^2.11.1",
    "standard-version": "^9.5.0",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
